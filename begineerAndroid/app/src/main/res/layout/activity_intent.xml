<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IntentActivity">

    <TextView
        android:id="@+id/judul_intent_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:text="@string/this_is_moveactivity"
        android:layout_margin="16dp"/>

    <TextView
        android:id="@+id/tv_string_intent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ini String Terkirim"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/judul_intent_activity"
        app:layout_constraintTop_toBottomOf="@id/judul_intent_activity" />
</androidx.constraintlayout.widget.ConstraintLayout>